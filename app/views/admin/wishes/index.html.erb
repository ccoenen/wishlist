<h1>Listing admin_wishes</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wishes.each do |wish| %>
  <tr>
    <td><%= wish.title %></td>
    <td><%= link_to 'Show', admin_wish_path(wish) %></td>
    <td><%= link_to 'Edit', edit_admin_wish_path(wish) %></td>
    <td><%= link_to 'Destroy', admin_wish_path(wish), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wish', new_admin_wish_path %>
