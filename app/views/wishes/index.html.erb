<h1>Listing wishes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Notes</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wishes.each do |wish| %>
  <tr>
    <td><%= image_tag wish.image.thumb.url.to_s %></td>
    <td><%= wish.title %></td>
    <td><%= wish.notes %></td>
    <td><%= wish.url %></td>
    <td><%= link_to 'Show', wish %></td>
  </tr>
<% end %>
</table>
